<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectgoob"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.projectgoob.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.projectgoob.AddAlarmActivity"
            android:label="@string/title_activity_add_alarm" >
        </activity>

        <service android:name="com.example.projectgoob.MyAlarmService" >
        </service>

        <activity
            android:name="com.example.projectgoob.MathActivity"
            android:label="@string/title_activity_math" >
        </activity>
        <activity
            android:name="com.example.projectgoob.AlarmList"
            android:label="@string/title_activity_alarm_list" >
        </activity>
        <activity
            android:name="com.example.projectgoob.AlarmListViewer"
            android:label="@string/title_activity_alarm_list_viewer" >
        </activity>
    </application>

</manifest>